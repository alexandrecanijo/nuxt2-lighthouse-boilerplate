{
  "name": "nuxt2-lighthouse-boilerplate",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:fix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install",
    "test": "jest",
    "outdated": "npx browserslist@latest --update-db && npx ncu",
    "updated": "npx ncu -u",
    "lighthouse:mobile": "lhci autorun",
    "lighthouse:desktop": "lhci autorun --collect.settings.preset=desktop"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "core-js": "^3.32.2",
    "nuxt": "^2.17.1",
    "vue": "^2.7.14",
    "vue-server-renderer": "^2.7.14",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^4.47.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@lhci/cli": "^0.12.0",
    "@nuxt/types": "~2.17.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.2.1",
    "@vue/test-utils": "^1.3.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.4",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.8",
    "prettier": "^2.8.8",
    "sass": "^1.68.0",
    "sass-loader": "^10.4.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^29.0.0",
    "vue-jest": "^3.0.7"
  }
}
